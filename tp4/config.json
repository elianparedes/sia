      "noise": 0.2
    },
    "pca": {
      "input_path": "/data/europe.csv",
      "weight_initializer": "random",
      "radius": 3,
      "learning_rate": 0.1
    }
  },
  "plots": {
    "oja_epochs": {
      "max_epochs": 10000,
      "learning_rate": 0.0001
    }
  }
}